	<div class="login-head">
	<h1>Login : </h1>
	</div>
	<div class="login-form">
<%= form_tag session_path do %>
<div class="field">
	<div class="field-text">
	<%= label_tag :email %> :
	</div>
	<div class="field-input">
	<%= text_field_tag :email %>
	</div>
</div>
<div class="field">
	<div class="field-text">
	<%= label_tag :password %> :
	</div>
	<div class="field-input">
	<%= password_field_tag :password %>
	</div>
</div>
<div class="center-button">
	<%= submit_tag 'Login' %>
</div>
<% end %>
</div>
